{
  "name": "data-url",
  "description": "Parses data: URLs",
  "keywords": [
    "data url",
    "data uri",
    "data:",
    "http",
    "fetch",
    "whatwg"
  ],
  "version": "1.0.0-pre",
  "author": "Domenic Denicola <d@domenic.me> (https://domenic.me/)",
  "license": "MIT",
  "repository": "jsdom/data-url",
  "main": "lib/parser.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "pretest": "node scripts/get-latest-platform-tests.js"
  },
  "devDependencies": {
    "eslint": "^4.13.0",
    "jest": "^21.2.1",
    "request": "^2.83.0"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "!/node_modules/abab/lib/atob.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ]
  },
  "dependencies": {
    "abab": "^1.0.4",
    "whatwg-mimetype": "^2.0.0",
    "whatwg-url": "^6.4.0"
  }
}
